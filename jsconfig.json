{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2016",
    "jsx": "preserve",
    "checkJs": true,
    "baseUrl": "./src",
    "paths": {
      "@redux/*": ["./src/@redux/*"],
      "components/*": ["./src/components/*"],
      "hooks/*": ["./src/hooks/*"],
      "page/*": ["./src/page/*"],
      "helpers/*": ["src/helpers/*"],
      "Layout/*": ["./src/Layout/*"]
    }
  },
  "include": ["src"],
  "exclude": ["node_modules", "**/node_modules/*"]
}
