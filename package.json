  "name": "Farsightproject",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@redux-offline/redux-offline": "^2.6.0",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^13.1.9",
    "axios": "^0.21.1",
    "bootstrap": "^5.0.1",
    "classnames": "^2.3.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.0.0",
    "exifreader": "^3.15.0",
    "express": "^4.17.1",
    "express-mysql-session": "^2.1.6",
    "express-session": "^1.17.2",
    "formik": "^2.2.8",
    "history": "^5.0.0",
    "http-proxy-middleware": "^2.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.1",
    "node-mysql": "^0.4.2",
    "node-sass": "^6.0.0",
    "nodemon": "^2.0.7",
    "pouchdb": "^7.2.2",
    "react": "^17.0.2",
    "react-bootstrap": "^1.6.0",
    "react-burger-menu": "^3.0.6",
    "react-datetime": "^3.0.4",
    "react-dom": "^17.0.2",
    "react-images-upload": "^1.2.8",
    "react-redux": "^7.2.4",
    "react-render-html": "^0.6.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "redux": "^4.1.0",
    "redux-devtools-extension": "^2.13.9",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-persist-pouchdb": "^0.2.1",
    "redux-pouchdb": "^1.0.0-rc.3",
    "redux-promise-middleware": "^6.1.2",
    "redux-thunker": "^0.2.5",
    "save": "^2.4.0",
    "semantic-ui-react": "^2.0.3",
    "styled-components": "^5.3.0",
    "uuid": "^8.3.2",
    "yup": "^0.32.9"
  },
  "scripts": {
    "start": "npm-run-all client:build server:prod",
    "dev": "npm-run-all -p client:dev server:dev",
    "server:dev": "cross-env NODE_ENV='development' nodemon server/index.js",
    "server:prod": "cross-env NODE_ENV='production' node server/index.js",
    "client:build": "react-scripts build",
    "client:dev": "react-scripts start",
    "test": "react-scripts test",
    "lint": "eslint --ext .jsx --ext .js src/.",
    "lint:fix": "eslint --fix --ext .jsx --ext .js src/.",
    "format": "prettier --write \"**/*.+(js|jsx|json|yml|yaml|css|md)\"",
    "eject": "react-scripts eject",
    "prepare": "husky install"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "express-pino-logger": "^6.0.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "node-env-run": "^4.0.2",
    "npm-run-all": "^4.1.5",
    "pino-colada": "^2.1.0",
    "prettier": "^2.3.0"
  },
  "lint-staged": {
    "src/**/*.+(js|jsx)": [
      "npm run lint:fix"
    ],
    "src/**/*.+(json|yml|yaml|css|md)": "npm run format"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-merge": "./.husky/post-merge"
    }
  }
}
